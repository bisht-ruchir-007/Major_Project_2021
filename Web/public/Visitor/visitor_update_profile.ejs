    <section id="hero" class="d-flex align-items-center">
      <div class="container">
        <div class="row">
          <div
            class="col-lg-6 d-flex flex-column justify-content-center pt-4 pt-lg-0 order-2 order-lg-1"
            data-aos="fade-up"
            data-aos-delay="200"
          >
            <center>
              <h1>Update User Profile</h1>
              
              <form style="width: 70%" method="POST" action="/updateProfile_visitor">
                <div class="form-row">
                  <div class="form-group col-md-4">
                    <label for="firstname">First Name *</label>
                    <input
                      type="text"
                      class="form-control"
                      id="firstname"
                      name="firstname"
                      value="<%= user_data.firstname %>"
                      required
                      disabled
                    />
                  </div>
                  <div class="form-group col-md-4">
                    <label for="middlename">Middle Name</label>
                    <input
                      type="text"
                      class="form-control"
                      id="middlename"
                      name="middlename"
                      placeholder="optional"
                      value="<%= user_data.middlename %>"
                    />
                  </div>
                  <div class="form-group col-md-4">
                    <label for="lastname">Last Name * </label>
                    <input
                      type="text"
                      class="form-control"
                      id="lastname"
                      name="lastname"
                      value="<%= user_data.lastname %>"
                      required
                      disabled
                    />
                  </div>
                </div>
                <div class="form-group">
                  <label for="visiting_officier_email">Email address</label>
                  <input
                    type="email"
                    class="form-control"
                    id="visiting_officier_email"
                    name="email"
                    aria-describedby="emailHelp"
                    value="<%= user_data.gmailID %>"
                    required
                    disabled
                  />
                </div>
                <div class="form-row">
                    <div class="form-group col-md-4">
                        <label for="age">Age</label>
                        <input
                        type="number"
                        class="form-control"
                        id="age"
                        name="age"
                        value="<%= user_data.age %>"
                        pattern="[0-9]{2}"
                        max="65"
                        min="25"
                        placeholder="eg. 35"
                        />
                    </div>
                    <div class="form-group col-md-8">
                        <label for="contactNumber">Contact Number</label>
                        <input
                        type="tel"
                        class="form-control"
                        id="contactNumber"
                        name="contactNumber"
                        value="<%= user_data.contactNumber %>"
                        pattern="[0-9]{3}[0-9]{3}[0-9]{4}"
                        placeholder="eg. 2344566789"
                        />
                    </div>
                </div>

                <button type="submit" class="btn btn-success">
                  Submit
                </button>
                <a href="/visitor_Profile"> <button type="button" class="btn btn-danger">
                  Cancel
                </button></a>
              </form>
            </center>
