      <!-- ======= About Us Section ======= -->
      <section class="top-college">
        <div class="container" data-aos="fade-up">
          <div class="section-title">
            <h2>Most visited colleges</h2>
            <% for( let i = 0; i < data.length ; i++) { %>
            <hr class="w-4" />
            <% if(i%2==0) { %>
            <div class="row m-2">
              <div class="col-md-5">
                <img
                  class="featurette-image img-fluid mx-auto"
                  src="../assets/img/c2.png"
                  alt="Generic placeholder image"
                />
              </div>
              <div class="col-md-7 p-2">
                <b
                  ><h3 style="color: darkslategrey">
                    <%= data[i].CollegeName %>
                  </h3></b
                >
                <br />
                <span class="badge badge-pill badge-dark"
                  >ID - <%= data[i].C_id %></span
                >
                <br />
                <div class="p-3">
                  <h5>College Type - <%= data[i].type %></h5>
                  <h5>College Address - <%= data[i].address %></h5>
                  <br />
                  <a
                    href="/college_Profile?id=<%= data[i].id%>"
                    class="btn btn-sm btn-success"
                    >Profile
                    <span class="badge badge-light"
                      ><%= data[i].views.length%></span
                    ></a
                  >
                </div>
              </div>
            </div>
            <% } %> <% if(i%2!=0) { %>
            <div class="row">
              <div class="col-md-7 p-3">
                <b
                  ><h3 style="color: darkslategrey">
